{
  "name": "@jamashita/catacombe-request",
  "version": "3.12.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:jamashita/catacombe.git/packages/request"
  },
  "license": "MIT",
  "author": "jamashita <jamashita@arachnid.xyz>",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn clean && concurrently \"npm:build:*\"",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist && rimraf coverage",
    "test": "jest test",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install",
    "update:upgrade": "ncu --upgrade --timeout 600000"
  },
  "dependencies": {
    "@jamashita/anden-error": "1.5.0",
    "@jamashita/anden-type": "1.5.0",
    "@jamashita/steckdose-json": "1.0.0",
    "node-fetch": "3.0.0-beta.9"
  },
  "devDependencies": {
    "http-status-codes": "2.1.4",
    "nock": "13.0.11"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  }
}
